{
  "name": "contextgrade",
  "version": "0.1.0",
  "description": "Decision intelligence microservice for B2B SaaS onboarding and pricing",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:migrate:sandbox": "dotenv -e .env.sandbox -- prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.prod -- prisma migrate deploy",
    "db:push:sandbox": "dotenv -e .env.sandbox -- prisma db push",
    "db:push:prod": "dotenv -e .env.prod -- prisma db push",
    "db:studio:sandbox": "dotenv -e .env.sandbox -- prisma studio",
    "db:studio:prod": "dotenv -e .env.prod -- prisma studio",
    "dev:sandbox": "dotenv -e .env.sandbox -- tsx watch src/server.ts"
  },
  "keywords": [
    "decision-intelligence",
    "microservice",
    "context-grade"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@prisma/client": "^5.19.1",
    "dotenv": "^17.2.4",
    "fastify": "^4.26.2",
    "openai": "^4.52.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "dotenv-cli": "^11.0.0",
    "prisma": "^5.19.1",
    "supabase": "^2.75.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
