{
  "name": "contextgrade",
  "version": "0.1.0",
  "description": "Decision intelligence microservice for B2B SaaS onboarding and pricing",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "dev:sandbox": "dotenv -e .env.sandbox -- tsx watch src/server.ts",
    "dev:worker": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:preview": "wrangler deploy --env preview",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:migrate:sandbox": "dotenv -e .env.sandbox -- prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.prod -- prisma migrate deploy",
    "db:push:sandbox": "dotenv -e .env.sandbox -- prisma db push",
    "db:push:prod": "dotenv -e .env.prod -- prisma db push",
    "db:studio:sandbox": "dotenv -e .env.sandbox -- prisma studio",
    "db:studio:prod": "dotenv -e .env.prod -- prisma studio",
    "cf-typegen": "wrangler types"
  },
  "keywords": [
    "decision-intelligence",
    "microservice",
    "context-grade"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/adapter-pg": "^6.19.2",
    "@prisma/client": "^6.19.2",
    "@supabase/supabase-js": "^2.95.3",
    "dotenv": "^17.2.4",
    "fastify": "^4.26.2",
    "jose": "^6.1.3",
    "openai": "^4.52.7",
    "pg": "^8.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250224.0",
    "@types/node": "^20.14.12",
    "@types/pg": "^8.11.10",
    "dotenv-cli": "^11.0.0",
    "prisma": "^6.19.2",
    "supabase": "^2.75.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "wrangler": "^4.0.0"
  }
}
